<% friends.each(function(friend) { %>
  <div class="row">
    <div class="small-3 large-2 columns ">
      <img src="<%= friend.get('profile_img_url') %>" />
    </div>
    <div class="small-9 large-10 columns">
      <p><strong><%= friend.get("name") %></strong> 
      <br>
      <%= friend.get("email") %>
    </div>
  </div>
  <hr />
<% }); %>